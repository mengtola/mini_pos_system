<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pos.domain.Customers" table="customers" catalog="mini_pos_system">
        <id name="cusId" type="java.lang.Integer">
            <column name="cus_id" />
            <generator class="identity" />
        </id>
        <property name="cusName" type="string">
            <column name="cus_name" not-null="true" />
        </property>
        <property name="cusPhone" type="string">
            <column name="cus_phone" not-null="true" />
        </property>
    </class>
</hibernate-mapping>