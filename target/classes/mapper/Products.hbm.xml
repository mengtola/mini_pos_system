<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pos.domain.Products" table="products" catalog="mini_pos_system">
        <id name="proId" type="java.lang.Integer">
            <column name="pro_id" />
            <generator class="identity" />
        </id>
        <property name="proName" type="string">
            <column name="pro_name" not-null="true" />
        </property>
        <property name="proCode" type="string">
            <column name="pro_code" not-null="true" />
        </property>
        <property name="qty" type="java.lang.Integer">
            <column name="qty" not-null="true" />
        </property>
         <property name="unitPrice" type="java.lang.Integer">
            <column name="unit_price" not-null="true" />
        </property>
         <property name="proStatus" type="boolean">
            <column name="pro_status" not-null="true" />
        </property>
        <!-- many-to-one name="category" class="com.pos.domain.Categories" fetch="select">
            <column name="cat_id" not-null="true" />
        </many-to-one>
        <many-to-one name="brand" class="com.pos.domain.Brands" fetch="select">
            <column name="bra_id" not-null="true" />
        </many-to-one> -->
    </class>
</hibernate-mapping>